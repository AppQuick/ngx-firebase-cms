{
  "name": "ngx-firebase-cms",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-firebase-cms-tester",
    "npm-pack": "cd dist/ngx-firebase-cms && npm pack",
    "copy-license": "cp LICENSE dist/ngx-firebase-cms",
    "copy-readme": "cp README.md dist/ngx-firebase-cms",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "build-lib": "ng build ngx-firebase-cms",
    "watch-lib": "ng build ngx-firebase-cms --watch",
    "build-app": "ng build ngx-firebase-cms-tester --prod",
    "test-lib": "ng test ngx-firebase-cms",
    "test-app": "ng test ngx-firebase-cms-tester",
    "patch": "npm --no-git-tag-version version patch && cd projects/ngx-firebase-cms && npm --no-git-tag-version version patch",
    "minor": "npm --no-git-tag-version version minor && cd projects/ngx-firebase-cms && npm --no-git-tag-version version minor",
    "major": "npm --no-git-tag-version version major && cd projects/ngx-firebase-cms && npm --no-git-tag-version version major",
    "package": "npm run build-lib && npm run copy-files && npm run npm-pack",
    "publish": "npm run build-lib && npm run copy-files && cd dist/ngx-firebase-cms && npm publish",
    "after-npm": "git commit -am \"deploy version to npm\" && git push",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.9",
    "@angular/common": "~7.2.9",
    "@angular/compiler": "~7.2.9",
    "@angular/core": "~7.2.9",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "~7.2.9",
    "@angular/platform-browser": "~7.2.9",
    "@angular/platform-browser-dynamic": "~7.2.9",
    "@angular/router": "~7.2.9",
    "core-js": "^2.5.4",
    "firebase": "^5.8.6",
    "lodash": "^4.17.11",
    "ng-zorro-antd": "^7.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.9",
    "@angular/language-service": "~7.2.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "^3.2.4"
  }
}
